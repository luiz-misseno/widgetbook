{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "sandbox",
      "cwd": "sandbox",
      "request": "launch",
      "program": "lib/widgetbook.dart",
      "type": "dart"
    },
    {
      "name": "cli",
      "cwd": "packages/widgetbook_cli",
      "request": "launch",
      "program": "bin/widgetbook.dart",
      "type": "dart",
      "args": [
        "cloud",
        "build",
        "push",
        "--api-key",
        "xxx",
        "--api-url",
        "http://localhost:3001/",
        "--path",
        "${workspaceFolder}/sandbox"
      ]
    },
    {
      "name": "full_example (default)",
      "cwd": "examples/full_example",
      "request": "launch",
      "program": "lib/widgetbook.dart",
      "type": "dart"
    },
    {
      "name": "tractian-flutter-ui",
      "cwd": "examples/tractian-flutter-ui/widgetbook",
      "request": "launch",
      "type": "dart"
    }
  ]
}
